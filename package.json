{
    "name": "json-api-generator",
    "version": "1.0.0",
    "description": "A generator for json api with GUI.",
    "main": "index.js",
    "scripts": {
        "start": "webpack-dev-server --config build/webpack.dev.conf.js",
        "build": "webpack --config build/webpack.prod.conf.js",
        "commit": "git-cz",
        "precommit": "lint-staged",
        "prettier": "prettier --write src/**/*.{js,ts,tsx,jsx}",
        "test": "echo \"Error: no test specified\" && exit 1"
    },
    "author": {
        "name": "leoytliu",
        "email": "liuyitao811@hotmail.com",
        "url": "https://leoeatle.github.io/techBlog/"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/LeoEatle/json-api-generator.git"
    },
    "engines": {
        "npm": ">= 4.0.0"
    },
    "jest": {
        "testEnvironment": "node",
        "coverageDirectory": "coverage"
    },
    "lint-staged": {
        "*.js": [
            "eslint --fix",
            "git add"
        ],
        "*.json": [
            "prettier --write",
            "git add"
        ]
    },
    "eslintConfig": {
        "extends": [
            "prettier"
        ],
        "env": {
            "jest": true,
            "node": true,
            "es6": true
        },
        "rules": {
            "prettier/prettier": "warn"
        },
        "plugins": [
            "prettier"
        ]
    },
    "config": {
        "commitizen": {
            "path": "./node_modules/cz-conventional-changelog"
        }
    },
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/LeoEatle/json-api-generator/issues"
    },
    "homepage": "https://github.com/LeoEatle/json-api-generator#readme",
    "devDependencies": {
        "@babel/core": "^7.0.0",
        "@babel/plugin-proposal-class-properties": "^7.0.0",
        "@babel/plugin-proposal-decorators": "^7.2.3",
        "@babel/plugin-transform-runtime": "^7.1.0",
        "@babel/preset-env": "^7.0.0",
        "@babel/preset-react": "^7.0.0",
        "@babel/preset-typescript": "^7.1.0",
        "@babel/runtime": "^7.0.0",
        "autoprefixer-loader": "^3.2.0",
        "babel-loader": "^8.0.0",
        "clean-webpack-plugin": "^1.0.0",
        "commitizen": "^3.0.5",
        "commitlint": "^7.2.1",
        "coveralls": "^3.0.2",
        "css-loader": "^1.0.0",
        "eslint": "^5.12.0",
        "eslint-config-prettier": "^3.3.0",
        "eslint-plugin-prettier": "^3.0.1",
        "file-loader": "^2.0.0",
        "fork-ts-checker-webpack-plugin": "^0.5.2",
        "friendly-errors-webpack4-plugin": "^2.0.1",
        "html-loader": "^0.5.1",
        "html-webpack-plugin": "^4.0.0-beta.1",
        "html-webpack-pos-plugin": "^1.0.1",
        "husky": "^1.3.1",
        "jest": "^23.6.0",
        "less": "^3.8.0",
        "less-loader": "^4.1.0",
        "lint-staged": "^8.1.0",
        "mini-css-extract-plugin": "^0.4.3",
        "optimize-css-assets-webpack-plugin": "^5.0.1",
        "osenv": "^0.1.5",
        "postcss-loader": "^3.0.0",
        "prettier": "^1.15.3",
        "raw-loader": "^0.5.1",
        "react-hot-loader": "^4.6.3",
        "replace-text-loader": "^0.2.1",
        "style-loader": "^0.23.1",
        "thread-loader": "^1.2.0",
        "tslint": "^5.12.1",
        "typings-for-css-modules-loader": "^1.7.0",
        "uglify-js": "^2.8.29",
        "uglifyjs-webpack-plugin": "^2.0.1",
        "url-loader": "^1.1.2",
        "webpack": "^4.20.2",
        "webpack-cli": "^3.2.1",
        "webpack-dev-server": "^3.1.14",
        "webpack-merge": "^4.2.1"
    },
    "dependencies": {
        "react": "^16.7.0",
        "react-dom": "^16.7.0",
        "@types/jest": "23.3.12",
        "@types/node": "10.12.18",
        "@types/react": "16.7.19",
        "@types/react-dom": "16.0.11",
        "typescript": "3.2.2"
    }
}
